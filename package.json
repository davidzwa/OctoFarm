    "systeminformation": "^5.6.12",
    "wake_on_lan": "^1.0.0",
    "winston": "^3.3.3",
    "ws": "^7.4.6",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.1",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
