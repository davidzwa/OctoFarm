      "views/**/*"
    ]
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.10.0"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "views/assets/*",
      "logs/*",
      "client_src/*"
    ],
    "delay": 2500
  }
}
