{
  "name": "octofarm",
  "version": "1.1.14-rc1",
  "description": "OctoFarm is a easy to setup and install web interface that unifies your Octoprint instances for easy monitoring of all your printers. OctoFarm allows you to connect to multiple octoprint instances on your network allowing creating a farm management system. You can control, manage, upload files to any of your instances without ever leaving the tab it's open in.",
  "main": "app.js",
  "scripts": {
    "prestart": "npm ci --production",
    "start": "pm2 flush && pm2 start app.js --name OctoFarm -e ./logs/pm2.error.log -o ./logs/pm2.out.log --time --restart-delay=1000 --update-env --exp-backoff-restart-delay=1500",
    "restart": "pm2 restart OctoFarm",
    "pupdate": "pm2 update",
    "stop": "pm2 stop OctoFarm",
    "stop:delete": "pm2 delete OctoFarm",
    "prerelease": "gulp",
    "release": "git archive -o octofarm.zip HEAD",
    "package": "pkg --compress --out-path package package.json",
    "prettier": "prettier --write .",
    "dev": "nodemon ./app.js",
    "test": "jest --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OctoFarm/OctoFarm"
  },
  "bin": {
    "octofarm": "./app.js"
  },
  "keywords": [
    "octofarm",
    "octo",
    "octoprint",
    "pi",
    "node"
  ],
  "author": "James Mackay (NotExpectedYet) & David Zwart (davidzwa)",
  "license": "ISC",
  "dependencies": {
    "apexcharts": "^3.24.0",
    "archiver": "^5.3.0",
    "bcryptjs": "^2.4.3",
    "bootbox": "^5.4.0",
    "bootstrap": "^4.3.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^9.0.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.1",
    "flatted": "^2.0.2",
    "gridstack": "^4.2.2",
    "influx": "^5.9.0",
    "is-docker": "^2.1.1",
    "jplist-es6": "^1.2.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.21",
    "lookpath": "^1.2.0",
    "luxon": "^1.26.0",
    "mjpeg-decoder": "^1.0.3",
    "mongoose": "^5.12.9",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "node-upnp-ssdp": "^0.1.1",
    "nodemon": "^2.0.7",
    "noty": "^3.2.0-beta",
    "passport": "^0.4.1",
